package com.wgc.green.manager;

import java.util.*;

import org.springframework.beans.factory.annotation.Autowired;

import com.wgc.green.entity.SensorData;
import com.wgc.green.repository.SensorDataRepository;

public class SensorDataManager {
	@Autowired
	private SensorDataRepository sensorDataRepository;
	
	public static List<SensorData> getSensorDataByIDList(List<Long> sensorIDList) {
		List<SensorData> sensorDataList = new LinkedList<>();
		if(sensorIDList == null) { return sensorDataList;}

		for(Long l : sensorIDList) {
			this.sensorDataRepository.findByID(l);
		}
	}

}
